package com.xjw.entity.form.user;

import com.xjw.entity.form.BaseForm;

public class MemberAmountForm extends BaseForm{
	private static final long serialVersionUID = 7098982607384170172L;

	/** 时间范围类别 */
	private Integer dayType;
	
	private String beginTime;
	
	private String endTime;
	
	private String vipType;//VIP等级

	public Integer getDayType() {
		return dayType;
	}

	public void setDayType(Integer dayType) {
		this.dayType = dayType;
	}

	public String getBeginTime() {
		return beginTime;
	}

	public void setBeginTime(String beginTime) {
		this.beginTime = beginTime;
	}

	public String getEndTime() {
		return endTime;
	}

	public void setEndTime(String endTime) {
		this.endTime = endTime;
	}

	public String getVipType() {
		return vipType;
	}

	public void setVipType(String vipType) {
		this.vipType = vipType;
	}
	
}
