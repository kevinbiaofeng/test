package com.xjw.entity.po.activity;

import java.math.BigDecimal;

import com.xjw.base.entity.BasePo;

/**
 * 开宝箱-全民签到
 */
public class Activity39 extends BasePo {
	private static final long serialVersionUID = -4688038561839796821L;

	/** 用户ID */
	private Long userId;
	/** 今日存款金额 */
	private BigDecimal depositAmount;
	/** 第几个礼盒*/
	private Integer boxIndex;
	/** 礼盒赠送金额 */
	private BigDecimal boxAmount;
	/** 礼盒提现积分 */
	private BigDecimal boxIntegral;
	/** 礼盒提现倍数 */
	private Integer boxMultiple;
	/** 备注 */
	private String remark;

	public Long getUserId() {
		return userId;
	}

	public void setUserId(Long userId) {
		this.userId = userId;
	}

	public BigDecimal getDepositAmount() {
		return depositAmount;
	}

	public void setDepositAmount(BigDecimal depositAmount) {
		this.depositAmount = depositAmount;
	}

	public Integer getBoxIndex() {
		return boxIndex;
	}

	public void setBoxIndex(Integer boxIndex) {
		this.boxIndex = boxIndex;
	}

	public BigDecimal getBoxAmount() {
		return boxAmount;
	}

	public void setBoxAmount(BigDecimal boxAmount) {
		this.boxAmount = boxAmount;
	}

	public BigDecimal getBoxIntegral() {
		return boxIntegral;
	}

	public void setBoxIntegral(BigDecimal boxIntegral) {
		this.boxIntegral = boxIntegral;
	}

	public Integer getBoxMultiple() {
		return boxMultiple;
	}

	public void setBoxMultiple(Integer boxMultiple) {
		this.boxMultiple = boxMultiple;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}
}
